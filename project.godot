; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "References",
"language": "GDScript",
"path": "res://Resources/Reference.gd"
}, {
"base": "Resource",
"class": "SaveData",
"language": "GDScript",
"path": "res://Scripts/SaveData.gd"
} ]
_global_script_class_icons={
"References": "",
"SaveData": ""
}

[application]

config/name="Geo Cards"
run/main_scene="res://Scenes/Splash.tscn"
config/icon="res://icon.png"

[autoload]

Deck="*res://Scripts/Deck.gd"

[display]

window/size/width=213
window/size/height=120
window/size/test_width=1280
window/size/test_height=720
mouse_cursor/custom_image="res://Sprites/Cursor.png"
mouse_cursor/custom_image_hotspot=Vector2( 16, 16 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.243137, 0.137255, 0.278431, 1 )
environment/default_environment="res://default_env.tres"
